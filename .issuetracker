# Integration with Issue Tracker
#
# (note that '\' need to be escaped).

[issuetracker "GitHub Pull Request Rule"]
  regex = "pull request #(\\d+)"
  url = "https://github.com/SalesforceBrainz/sfmc-devtools-copado/pull/$1"

[issuetracker "GitHub Issue Rule"]
  regex = "#(\\d+)"
  url = "https://github.com/SalesforceBrainz/sfmc-devtools-copado/issues/$1"
