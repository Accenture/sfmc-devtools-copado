{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Apex Replay Debugger",
      "type": "apex",
      "request": "launch",
      "sfdxProject": "${workspaceRoot}"
    },
    {
      "type": "node-terminal",
      "name": "debug: mcdev.init",
      "request": "launch",
      "command": "node --preserve-symlinks McdevInit.fn.js",
      "cwd": "${workspaceFolder}/copado-function/dist",
      "envFile": "${workspaceFolder}/copado-function/app/environments/McdevInit.env",
      "preLaunchTask": "esbuild"
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_retrieve",
      "request": "launch",
      "command": "node --preserve-symlinks Retrieve.fn.js",
      "cwd": "${workspaceFolder}/copado-function/dist",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Retrieve.env",
      "preLaunchTask": "esbuild"
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_commit",
      "request": "launch",
      "command": "node --preserve-symlinks Commit.fn.js",
      "cwd": "${workspaceFolder}/copado-function/dist",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Commit.env",
      "preLaunchTask": "esbuild"
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_deploy",
      "request": "launch",
      "command": "node --preserve-symlinks Deploy.fn.js",
      "cwd": "${workspaceFolder}/copado-function/dist",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Deploy.env",
      "preLaunchTask": "esbuild"
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_upgrade",
      "request": "launch",
      "command": "node --preserve-symlinks Upgrade.fn.js",
      "cwd": "${workspaceFolder}/copado-function/dist",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Upgrade.env",
      "preLaunchTask": "esbuild"
    }
  ]
}