{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Apex Replay Debugger",
      "type": "apex",
      "request": "launch",
      "sfdxProject": "${workspaceRoot}"
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_retrieve",
      "request": "launch",
      "command": "npm run cop_retrieve",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Retrieve.env",
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_commit",
      "request": "launch",
      "command": "npm run cop_commit",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Commit.env",
    },
    {
      "type": "node-terminal",
      "name": "debug: cop_deploy",
      "request": "launch",
      "command": "npm run cop_deploy",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/copado-function/app/environments/Deploy.env",
    }
  ]
}